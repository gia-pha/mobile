{
  "consumer": {
    "name": "mobile"
  },
  "interactions": [
    {
      "description": "a request to check authentication status",
      "request": {
        "headers": {
          "Cookie": "session=abc12345"
        },
        "matchingRules": {
          "header": {
            "Cookie": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "session=[a-zA-Z0-9\\-_.]+"
                }
              ]
            }
          }
        },
        "method": "GET",
        "path": "/session"
      },
      "response": {
        "body": {
          "createdAt": "2023-10-01T12:34:56Z",
          "id": "123",
          "name": "John Doe",
          "updatedAt": "2023-10-01T12:34:56Z"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.createdAt": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.updatedAt": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request to check authentication status with no session",
      "request": {
        "method": "GET",
        "path": "/session"
      },
      "response": {
        "status": 401
      }
    },
    {
      "description": "a request to create login options",
      "request": {
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/login/start"
      },
      "response": {
        "body": {
          "challenge": "T1xCsnxM2DNL2KdK5CLa6fMhD7OBqho6syzInk_n-Uo",
          "rpId": "example.com",
          "timeout": 60000,
          "userVerification": "required"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    },
    {
      "description": "a request to create register options",
      "request": {
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/register/start"
      },
      "response": {
        "body": {
          "attestation": "none",
          "authenticatorSelection": {
            "residentKey": "required",
            "userVerification": "required"
          },
          "challenge": "3vTO0kB4v2T6xMEsIgO7Bg",
          "pubKeyCredParams": [
            {
              "alg": -7,
              "type": "public-key"
            },
            {
              "alg": -257,
              "type": "public-key"
            }
          ],
          "rp": {
            "id": "example.com",
            "name": "My App"
          },
          "timeout": 60000,
          "user": {
            "displayName": "Passkey User",
            "id": "QWJjMTIzNDU2",
            "name": "user@example.com"
          }
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    },
    {
      "description": "a request to finish login",
      "request": {
        "body": {
          "id": "ZGV2aWNlY3JlZGlk",
          "rawId": "ZGV2aWNlY3JlZGlk",
          "response": {
            "authenticatorData": "YXV0aGRhdGE",
            "clientDataJSON": "eyJjaGFsbGVuZ2UiOiAiLi4uIn0",
            "signature": "c2lnbmF0dXJl",
            "userHandle": "QWJjMTIzNDU2"
          },
          "type": "public-key"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/login/complete"
      },
      "response": {
        "body": {
          "status": "ok",
          "userId": "QWJjMTIzNDU2"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    },
    {
      "description": "a request to finish register",
      "request": {
        "body": {
          "id": "ZGV2aWNlY3JlZGlk",
          "rawId": "ZGV2aWNlY3JlZGlk",
          "response": {
            "attestationObject": "o2NmbXR...",
            "clientDataJSON": "eyJjaGFsbGVuZ2UiOiAiLi4uIn0"
          },
          "type": "public-key"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/register/complete"
      },
      "response": {
        "body": {
          "status": "ok",
          "userId": "QWJjMTIzNDU2"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.27",
      "mockserver": "1.2.11",
      "models": "1.2.8"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "authn"
  }
}