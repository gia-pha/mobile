{
  "consumer": {
    "name": "mobile"
  },
  "interactions": [
    {
      "description": "a request to check authentication status",
      "request": {
        "headers": {
          "Cookie": "session=abc12345"
        },
        "matchingRules": {
          "header": {
            "Cookie": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "session=[a-zA-Z0-9\\-_.]+"
                }
              ]
            }
          }
        },
        "method": "GET",
        "path": "/session"
      },
      "response": {
        "body": {
          "createdAt": "2023-10-01T12:34:56Z",
          "id": "123",
          "name": "John Doe",
          "updatedAt": "2023-10-01T12:34:56Z"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.createdAt": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.updatedAt": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request to check authentication status with no session",
      "request": {
        "method": "GET",
        "path": "/session"
      },
      "response": {
        "status": 401
      }
    },
    {
      "description": "a request to create login options",
      "request": {
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/login/start"
      },
      "response": {
        "body": {
          "challenge": "aBcDeFg123",
          "rpId": "example.com",
          "timeout": 60000,
          "userVerification": "required"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    },
    {
      "description": "a request to create register options",
      "request": {
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/register/start"
      },
      "response": {
        "body": {
          "attestation": "none",
          "authenticatorSelection": {
            "residentKey": "required",
            "userVerification": "required"
          },
          "challenge": "3vTO0kB4v2T6xMEsIgO7Bg",
          "pubKeyCredParams": [
            {
              "alg": -7,
              "type": "public-key"
            },
            {
              "alg": -257,
              "type": "public-key"
            }
          ],
          "rp": {
            "id": "example.com",
            "name": "My App"
          },
          "timeout": 60000,
          "user": {
            "displayName": "Passkey User",
            "id": "QWJjMTIzNDU2",
            "name": "user@example.com"
          }
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    },
    {
      "description": "a request to finish login",
      "request": {
        "body": {
          "id": "ZGV2aWNlY3JlZGlk",
          "rawId": "ZGV2aWNlY3JlZGlk",
          "response": {
            "authenticatorData": "YXV0aGRhdGE",
            "clientDataJSON": "eyJjaGFsbGVuZ2UiOiAiLi4uIn0",
            "signature": "c2lnbmF0dXJl",
            "userHandle": "QWJjMTIzNDU2"
          },
          "type": "public-key"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/login/complete"
      },
      "response": {
        "body": {
          "status": "ok",
          "userId": "QWJjMTIzNDU2"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    },
    {
      "description": "a request to finish register",
      "request": {
        "body": {
          "id": "ZGV2aWNlY3JlZGlk",
          "rawId": "ZGV2aWNlY3JlZGlk",
          "response": {
            "attestationObject": "o2NmbXR...",
            "clientDataJSON": "eyJjaGFsbGVuZ2UiOiAiLi4uIn0"
          },
          "type": "public-key"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/register/complete"
      },
      "response": {
        "body": {
          "status": "ok",
          "userId": "QWJjMTIzNDU2"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    },
    {
      "description": "a request to get digital asset links",
      "request": {
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "GET",
        "path": "/assetlinks.json"
      },
      "response": {
        "body": {
          "relation": [
            "delegate_permission/common.handle_all_urls",
            "delegate_permission/common.get_login_creds"
          ],
          "target": {
            "namespace": "android_app",
            "package_name": "com.example.android",
            "sha256_cert_fingerprints": [
              "c4bbcb1fbec99d65bf59d85c8cb62ee2db963f0fe106f483d9afa73bd4e39a8a"
            ]
          }
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 0
                }
              ]
            },
            "$.target.sha256_cert_fingerprints": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "[A-Fa-f0-9]{64}"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request to get robots",
      "request": {
        "method": "GET",
        "path": "/robots.txt"
      },
      "response": {
        "body": "\"User-agent: *\\nAllow: /.well-known/\\n\"",
        "headers": {
          "Content-Type": "text/plain"
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.27",
      "mockserver": "1.2.11",
      "models": "1.2.8"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "authn"
  }
}